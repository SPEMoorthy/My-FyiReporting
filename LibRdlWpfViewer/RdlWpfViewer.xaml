<UserControl x:Class="LibRdlWpfViewer.RdlWpfViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
    xmlns:myctrl="clr-namespace:fyiReporting.RdlViewer;assembly=RdlViewer" >
    <DockPanel LastChildFill="True" VerticalAlignment="Stretch" Margin="0">
        <WindowsFormsHost  Name="windowsFormsHost1" DockPanel.Dock="Top" VerticalAlignment="Stretch" Margin="0">
           <myctrl:RdlViewer x:Name="reportViewer" Dock="Fill" Margin="0,0,0,0" />
        </WindowsFormsHost>
    </DockPanel>
</UserControl>
